/* redirect/redirect.wxss */
.swiper-tab{  
  width: 100%;  
  height:80rpx;
  border-bottom: 2rpx solid #777777;  
  text-align: center;  
  line-height: 80rpx;
}  
.swiper-tab-list{  
  font-size: 30rpx;
  display: inline-block;
  width: 33.33%;
  color: #777777;  
}  
.on{ 
  color: #da7c0c;  
  border-bottom: 5rpx solid #da7c0c;
}  
.swiper-box{
  display: block; 
  width: 100%; 
  /* height:1000rpx!important; */
  overflow: hidden; 
}  
.swiper-box view{  
    text-align: center;  
}  
.box{
  height:1000rpx;
  background:rgba(99,0,0,0.3);
}
.lists{
  height:90rpx;
  line-height:90rpx;
  border-bottom:1rpx solid rgba(99,0,0,0.6);
}
.bosx{
  width:100%;
  height:100rpx;
  border:1px solid red;
  position: fixed;
  left:0;
  bottom:0;
}